{
  "name": "qt6renderervsc",
  "displayName": "Qt6RendererVsc",
  "description": "Visual Studio Code plugin for pretty printing Qt types by GDB and LLDB",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Debuggers",
    "Visualization",
    "Other"
  ],
  "activationEvents": [    
    "onDebugResolve:cppdbg",
    "onDebugResolve:lldb-dap"
  ],
  "main": "./dist/extension.js",
  "contributes": {},
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",        
    "lint": "prettier -c ./src && eslint ./src",
    "lint:fix": "prettier -w ./src && eslint --fix ./src"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.92.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "copy-webpack-plugin": "^12.0.2",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.0.1",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4"
  }
}
